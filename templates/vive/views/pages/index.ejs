        <section class="hero">
            <div class="hero__wrapper">
                <h2></h2>
                <p></p>
                <button></button>
                <base-button></base-button>
            </div>
        </section>

            <main class="container">
                <div class="jumbotron">
                    <h1>Top Mascots</h1>
