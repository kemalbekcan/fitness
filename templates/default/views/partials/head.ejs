<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>
    <%= head.title %>
</title>
<meta name="title" content="<%= head.title %>">
<meta name="author" content="<%= head.author %>">
<meta name="og:title" content="<%= head.title %>">
<meta name="og:description" content="<%= head.description %>">
<meta name="description" content="<%= head.description %>">
<meta name="keywords" content="<%= head.keywords %>">
<meta name="og:keywords" content="<%= head.keywords %>">
<meta name="theme-color" content="#000000">
<link rel="icon" href="/public/images/favicon.ico" type="image/x-icon">

<!-- Lazy loading CSS -->
<% cssFiles.forEach(cssFile => { %>
    <link id="lazy-css" rel="stylesheet" href="<%= cssFile %>" media="print" onload="this.media='all'">
<% }); %>

<noscript>
    <% cssFiles.forEach(cssFile => { %>
        <link rel="stylesheet" href="<%= cssFile %>">
    <% }); %>
</noscript>

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

<!-- Twitter -->
<!-- Facebook -->
<!-- Google -->

<link rel="canonical" href="http://localhost:3000">

<link rel="manifest" href="<%= manifestFile %>">

<!-- JavaScript ile lazy loading -->
<script defer>
    document.addEventListener('DOMContentLoaded', function () {
        var lazyStyle = document.getElementById('lazy-css');
        lazyStyle.removeAttribute('onload');
        lazyStyle.media = 'all';
    });
</script>
